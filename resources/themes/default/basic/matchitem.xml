<?xml version="1.0" encoding="UTF-8"?>
<Window size="285,80">
    <ListContainerElement class="listitem">
        <VBox height="100" mouse="false" padding="5,5,5,5">
            <HBox margin="0,3,0,0" mouse="false">
                <VBox width="110">
                    <Label name="team_player_name" singleline="true" align="left"
                        normaltextcolor="red" />
                    <Label name="team_player_level" singleline="true" align="left" />
                </VBox>

                <VBox width="85">
                    <Label text="本赛季:单双/灵活" normaltextcolor="blue" font="system_10" />
                    <Label name="team_player_rank_SOLO" normaltextcolor="blue" font="system_10" />
                    <Label name="team_player_rank_FLEX" normaltextcolor="blue" font="system_10" />
                </VBox>

                <VBox width="85">
                    <Label text="上赛季:单双/灵活" font="system_10" />
                    <Label name="team_player_rank_SOLO_last" font="system_10" />
                    <Label name="team_player_rank_FLEX_last" font="system_10" />
                </VBox>

            </HBox>


            <HBox margin="0,0,0,5" mouse="false" height="42">
                <Label name="熟练度top:" />
