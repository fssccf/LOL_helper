                <Control name="top2" halign="center" valign="bottom" width="40" height="40"
                    bordercolor="bk_wnd_lightcolor" bordersize="2,2,2,2" />
                <Control name="top3" halign="center" valign="bottom" width="40" height="40"
                    bordercolor="bk_wnd_lightcolor" bordersize="2,2,2,2" />
                <Control name="top4" halign="center" valign="bottom" width="40" height="40"
                    bordercolor="bk_wnd_lightcolor" bordersize="2,2,2,2" />
                <Control name="top5" halign="center" valign="bottom" width="40" height="40"
                    bordercolor="bk_wnd_lightcolor" bordersize="2,2,2,2" />
                <Control name="top6" halign="center" valign="bottom" width="40" height="40"
                    bordercolor="bk_wnd_lightcolor" bordersize="2,2,2,2" />
            </HBox>
        </VBox>
    </ListContainerElement>
</Window>